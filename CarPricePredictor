import pandas as pd 
df = pd.read_csv('carprice.csv')
from sklearn.model_selection import train_test_split
x= df[['Mileage','Age(yrs)']]
y= df['Sell Price($)']
x_train, x_test, y_train, y_test = train_test_split(x,y,test_size=0.8)
from sklearn.linear_model import LinearRegression
clf = LinearRegression()
clf.fit(x_train,y_train)
acc = clf.score(x_test,y_test)
print("The Accuracy of this model at this execution is: ",clf.score(x_test,y_test)*100)
a=float(input("Enter Mileage: "))
b=float(input("Enter Age(yrs): "))
c=[[a,b]]
newOutput = clf.predict(c)
print ("The Expected Price for the input values is RS:",newOutput)
